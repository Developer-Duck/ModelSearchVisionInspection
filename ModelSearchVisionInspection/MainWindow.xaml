                        <Ellipse Width="20" Height="20" Fill="#39FF14" Margin="15 0" ></Ellipse>
                    </StackPanel>
                    <StackPanel Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
                        <Button Width="30" Background="Transparent">
